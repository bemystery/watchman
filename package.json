    "build:test": "NODE_ENV=test easywebpack build test",
    "build:prod": "NODE_ENV=production easywebpack build prod",
    "deploy:test": "babel-node tools/deployTestServer.js",
    "start:prod": "ENABLE_NODE_LOG=YES EGG_SERVER_ENV=prod NODE_ENV=production egg-scripts start --port=7001 --daemon --title=egg-server-frontend-watchman",
    "start:test": "ENABLE_NODE_LOG=YES EGG_SERVER_ENV=test NODE_ENV=test egg-scripts start --port=7001 --daemon --title=egg-server-frontend-watchman",
    "stop": "egg-scripts stop --title=egg-server-frontend-watchman",
    "dev": "egg-bin dev --port 5000 --require @babel/register",
    "archive": "babel-node tools/archiveReleaseFile.js",
    "kill-port": "kill -9 $(lsof -i:8888 |grep 'node'|aw '{print $2}') ",
    "debug": "egg-bin debug --inspect --require @babel/register",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "ii": "npm install --registry https://registry.npm.taobao.org",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "migrate:new": "egg-sequelize migration:create",
    "migrate:up": "egg-sequelize db:migrate",
    "migrate:down": "egg-sequelize db:migrate:undo"
